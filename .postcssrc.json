{
  "parser": "postcss-scss",
  "map": true,
  "plugins": {
    "postcss-flexbugs-fixes": {},
    "postcss-preset-env": {
      "stage": 2,
      "autoprefixer": {
        "grid": true,
        "flexbox": true,
        "supports": true,
        "remove": true,
        "add": true,
        "cascade": true
      },
      "features": {
        "nesting-rules": true,
        "color-mod-function": { "unresolved": "warn" }
      }
    },
    "cssnano": {
      "preset": "default"
    }
  }
}
