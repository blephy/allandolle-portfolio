{
  "name": "strict-optimized-react-starter",
  "version": "1.0.0",
  "description": "Full typescript react webpack starter with strict development process. Secure NodeJs assets server. All web best practices implemented to have well performed SPA.",
  "main": "src/index.tsx",
  "scripts": {
    "start": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" NODE_ENV=\"development\" webpack-dev-server --color --watch --hot --stdin --config config/webpack/webpack.dev.ts",
    "start:prod": "cross-env NODE_ENV=\"production\" node server/index.ts",
    "build": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" NODE_ENV=\"production\" webpack --progress --colors --config config/webpack/webpack.prod.ts",
    "test": "jest --colors --config config/tests/jest.config.js",
    "test:watch": "jest --collectCoverage=false --colors --watch --config config/tests/jest.config.js",
    "ts:check": "tsc -p tsconfig.json --noEmit",
    "lint": "npm run eslint && npm run stylelint && npm run markdownlint && npm run prettier && npm run htmlhint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix && npm run markdownlint:fix && npm run prettier:fix",
    "eslint": "eslint --color --ext .jsx --ext .js --ext .ts --ext .tsx src/",
    "eslint:fix": "eslint --fix --color --ext .jsx --ext .js --ext .ts --ext .tsx src/",
    "stylelint": "stylelint \"**/*.{css,scss,less,sass,js,jsx,ts,tsx,html}\"",
    "stylelint:fix": "stylelint --fix \"**/*.{css,scss,less,sass,js,jsx,ts,tsx,html}\"",
    "markdownlint": "markdownlint \"**/*.md\"",
    "markdownlint:fix": "markdownlint --fix \"**/*.md\"",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "htmlhint": "htmlhint --config .htmlhintrc --ignore node_modules/ **/*.ejs **/*.html",
    "browsers": "browserslist",
    "browsers:coverage": "browserslist --coverage",
    "browsers:autoprefixer": "autoprefixer --info",
    "prestats": "mkdir -p stats && cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" NODE_ENV=\"production\" webpack --profile --json --config config/webpack/webpack.prod.ts > stats/stats.json",
    "stats": "webpack-bundle-analyzer stats/stats.json build/",
    "clean": "rm -rf stats && rm -rf reports && rm -rf coverage && rm -rf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blephy/strict-optimized-react-starter.git"
  },
  "keywords": [
    "react",
    "typescript",
    "eslint",
    "stylelint",
    "markdownlint",
    "htmlhint",
    "prettier",
    "webpack",
    "postcss",
    "svgo",
    "svgr",
    "babel",
    "lint-staged",
    "commitlint",
    "husky",
    "github actions",
    "normalize",
    "sanitize",
    "css modules",
    "lighthouse",
    "browserslist",
    "spa",
    "pwa"
  ],
  "author": {
    "name": "Allan Doll√©",
    "email": "hello@allandolle.fr"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blephy/strict-optimized-react-starter/issues"
