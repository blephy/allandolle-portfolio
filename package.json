{
  "name": "allandolle-portfolio",
  "version": "1.0.0",
  "description": "Allan Dolle's portfolio - React starter",
  "main": "src/index.tsx",
  "scripts": {
    "start": "webpack-dev-server --color --watch --hot --stdin --config config/webpack/webpack.dev.js",
    "start:prod": "node server/index.ts",
    "build": "webpack --progress --colors --config config/webpack/webpack.prod.js",
    "test": "jest --colors --config config/tests/jest.config.js",
    "test:watch": "jest --collectCoverage=false --colors --watch --config config/tests/jest.config.js",
    "ts:check": "tsc -p tsconfig.json --noEmit",
    "lint": "npm run eslint && npm run stylelint && npm run markdownlint && npm run prettier && npm run htmlhint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix && npm run markdownlint:fix && npm run prettier:fix",
    "lint:report": "mkdir -p reports && npm run eslint:report && npm run stylelint:report",
    "eslint": "eslint --color --ext .jsx --ext .js --ext .ts --ext .tsx src/",
    "eslint:fix": "eslint --fix --color --ext .jsx --ext .js --ext .ts --ext .tsx src/",
    "eslint:report": "eslint --fix --color --ext .jsx --ext .js --ext .ts --ext .tsx src/ -f json -o ./reports/eslint-report.json",
    "stylelint": "stylelint '**/*.{css,scss,less,sass,js,jsx,ts,tsx,html}'",
    "stylelint:fix": "stylelint --fix '**/*.{css,scss,less,sass,js,jsx,ts,tsx,html}'",
    "stylelint:report": "stylelint '**/*.{css,scss,less,sass,js,jsx,ts,tsx,html}' -f json -o ./reports/stylelint-report.json",
    "markdownlint": "markdownlint '**/*.md'",
    "markdownlint:fix": "markdownlint --fix '**/*.md'",
    "htmlhint": "htmlhint --config .htmlhintrc --ignore node_modules/ **/*.ejs **/*.html",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "browsers": "browserslist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blephy/allandolle-portfolio.git"
  },
  "keywords": [
    "react",
    "redux",
    "typescript",
    "eslint",
    "stylelint",
    "markdownlint",
    "htmlhint",
    "prettier",
    "webpack",
    "postcss",
    "svgo",
    "svgr",
    "babel",
    "lint-staged",
    "commitlint",
    "husky",
    "github actions"
  ],
  "author": {
    "name": "Allan Doll√©",
